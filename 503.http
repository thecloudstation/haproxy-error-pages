HTTP/1.0 503 Service Unavailable
Cache-Control: no-cache
Connection: close
Content-Type: text/html

<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width,initial-scale=1.0">
<meta http-equiv="refresh" content="10">
<title>503 - Service Unavailable | CloudStation</title>
<style>
:root{
--background:0 0% 3.9%;
--foreground:0 0% 98%;
--card:0 0% 3.9%;
--card-foreground:0 0% 98%;
--primary:25 95% 53%;
--primary-foreground:0 0% 100%;
--secondary:0 0% 14.9%;
--secondary-foreground:0 0% 98%;
--muted-foreground:0 0% 63.9%;
--border:0 0% 14.9%;
--destructive:0 62.8% 30.6%;
--destructive-foreground:0 0% 98%;
--radius:0.75rem;
--logo-color:#fafafa;
}
*{margin:0;padding:0;box-sizing:border-box;border-color:hsl(var(--border))}
body{
font-family:ui-sans-serif,system-ui,-apple-system,BlinkMacSystemFont,"Segoe UI",sans-serif;
background:hsl(var(--background));
color:hsl(var(--foreground));
min-height:100vh;
overflow:hidden;
position:relative;
-webkit-font-smoothing:antialiased;
-moz-osx-font-smoothing:grayscale;
}
.grid-bg{
position:absolute;
inset:0;
opacity:0.02;
background-image:linear-gradient(hsl(var(--foreground)) 1px,transparent 1px),linear-gradient(90deg,hsl(var(--foreground)) 1px,transparent 1px);
background-size:80px 80px;
}
.scan-line{
position:absolute;
inset:0;
overflow:hidden;
pointer-events:none;
}
.scan-line::after{
content:'';
position:absolute;
inset-inline:0;
height:1px;
background:hsl(var(--primary)/.2);
animation:scan 4s linear infinite;
}
.top-glow{
position:fixed;
top:-7rem;
left:50%;
height:18rem;
width:34rem;
transform:translateX(-50%);
border-radius:9999px;
background:radial-gradient(closest-side,rgba(238,106,12,0.18),transparent);
filter:blur(48px);
opacity:0.7;
pointer-events:none;
z-index:1;
}
@keyframes scan{
0%{transform:translateY(-100%)}
100%{transform:translateY(100vh)}
}
@keyframes pulse-slow{
0%,100%{opacity:0.4}
50%{opacity:1}
}
@keyframes slide-up{
from{opacity:0;transform:translateY(20px)}
to{opacity:1;transform:translateY(0)}
}
.header{
position:relative;
z-index:10;
display:flex;
align-items:center;
justify-content:space-between;
padding:1.25rem 1.5rem;
}
.header-left{
display:flex;
align-items:center;
gap:0.75rem;
}
.logo-mark{
width:14rem;
height:2rem;
display:block;
color:var(--logo-color);
flex-shrink:0;
}
.logo-mark path{
fill:currentColor;
}
.pulse-dot{
height:0.5rem;
width:0.5rem;
border-radius:9999px;
background:hsl(var(--primary));
animation:pulse-slow 3s ease-in-out infinite;
}
.status-label{

font-size:0.9rem;
letter-spacing:0.1em;
color:hsl(var(--muted-foreground));
text-transform:uppercase;
}
.time{

font-size:0.9rem;
color:hsl(var(--muted-foreground));
}
.main{
position:relative;
z-index:10;
display:flex;
flex-direction:column;
align-items:center;
justify-content:center;
padding:1.5rem;
min-height:100vh;
}
.content{
width:100%;
max-width:48rem;
}

font-size:0.9rem;
letter-spacing:0.3em;
color:hsl(var(--primary));
text-transform:uppercase;
margin-bottom:1.5rem;
opacity:0;
animation:slide-up 0.8s ease-out 0.1s forwards;
}
.error-code{
font-size:clamp(5rem,15vw,12rem);
font-weight:700;
line-height:0.85;
letter-spacing:-0.05em;
color:hsl(var(--foreground));
opacity:0;
animation:slide-up 0.8s ease-out 0.2s forwards;
}
.separator{
display:flex;
align-items:center;
gap:1rem;
margin:2rem 0;
opacity:0;
animation:slide-up 0.8s ease-out 0.3s forwards;
}
.separator .line{
height:1px;
flex:1;
background:hsl(var(--border));
}
.separator .diamond{
height:0.375rem;
width:0.375rem;
transform:rotate(45deg);
background:hsl(var(--primary));
}
.separator .accent-line{
height:1px;
width:4rem;
background:hsl(var(--primary)/.5);
}
.title{
font-size:1.7rem;
font-weight:600;
letter-spacing:-0.025em;
color:hsl(var(--foreground));
margin-bottom:0.75rem;
opacity:0;
animation:slide-up 0.8s ease-out 0.4s forwards;
}
.desc{
font-size:0.875rem;
line-height:1.6;
color:hsl(var(--muted-foreground));
max-width:45rem;
opacity:0;
animation:slide-up 0.8s ease-out 0.4s forwards;
}
.card{
margin-top:2rem;
border-radius:var(--radius);
border:1px solid hsl(var(--border));
background:hsl(var(--card));
padding:1.25rem;
opacity:0;
animation:slide-up 0.8s ease-out 0.5s forwards;
box-shadow:0 1px 3px 0 rgb(0 0 0 / 0.03);
}
.card-header{
display:flex;
align-items:center;
gap:0.5rem;
margin-bottom:0.75rem;
}
.card-dot{
height:0.375rem;
width:0.375rem;
border-radius:9999px;
background:hsl(var(--primary));
}
.card-label{

font-size:0.6875rem;
letter-spacing:0.1em;
color:hsl(var(--muted-foreground));
text-transform:uppercase;
}
.card-text{

font-size:0.9rem;
line-height:1.6;
color:hsl(var(--muted-foreground));
}
.visual{
margin-top:1.5rem;
border-radius:var(--radius);
border:1px solid hsl(var(--border));
background:hsl(var(--card));
padding:1.25rem;
opacity:0;
animation:slide-up 0.8s ease-out 0.6s forwards;
box-shadow:0 1px 3px 0 rgb(0 0 0 / 0.03);
}
.visual-header{
display:flex;
align-items:center;
gap:0.5rem;
margin-bottom:1rem;
}
.visual-dot{
height:0.375rem;
width:0.375rem;
border-radius:9999px;
background:hsl(var(--primary));
animation:pulse-slow 3s ease-in-out infinite;
}
.progress-container{
display:flex;
flex-direction:column;
gap:0.75rem;
}
.progress-header{
display:flex;
align-items:center;
justify-content:space-between;
}
.progress-label{

font-size:0.65rem;
color:hsl(var(--muted-foreground));
text-transform:uppercase;
}
.progress-value{

font-size:0.75rem;
color:hsl(var(--primary));
}
.progress-bar-bg{
height:0.25rem;
width:100%;
border-radius:9999px;
background:hsl(var(--border));
overflow:hidden;
}
.progress-bar{
height:100%;
border-radius:9999px;
background:hsl(var(--primary));
transition:width 0.2s ease-out;
}
.progress-steps{
display:flex;
align-items:center;
justify-content:space-between;
}
.step{

font-size:0.5625rem;
letter-spacing:0.05em;
color:hsl(var(--muted-foreground)/.4);
transition:color 0.5s ease;
}
.step.active{
color:hsl(var(--primary));
}
.actions{
display:flex;
flex-wrap:wrap;
align-items:center;
gap:1rem;
margin-top:2.5rem;
opacity:0;
animation:slide-up 0.8s ease-out 0.7s forwards;
}
.btn{
display:inline-flex;
align-items:center;
gap:0.5rem;
border-radius:var(--radius);
background:hsl(var(--primary));
color:hsl(var(--primary-foreground));
padding:0.75rem 1.25rem;
font-size:0.875rem;
font-weight:500;
text-decoration:none;
border:none;
cursor:pointer;
transition:all 0.2s;
font-family:inherit;
}
.btn:hover{opacity:0.9}
.btn-secondary{
background:hsl(var(--secondary));
color:hsl(var(--foreground));
border:1px solid hsl(var(--border));
}
.btn-secondary:hover{border-color:hsl(var(--primary)/.4);background:hsl(var(--card)/.8)}
.footer{
position:relative;
z-index:10;
display:flex;
align-items:center;
justify-content:space-between;
padding:1.25rem 1.5rem;
}
.footer-text{

font-size:0.625rem;
letter-spacing:0.1em;
color:hsl(var(--muted-foreground)/.6);
text-transform:uppercase;
}
@media(min-width:768px){
.header{padding:1.25rem 3rem}
.main{padding:1.5rem 3rem}
.footer{padding:1.25rem 3rem}
.top-glow{width:72rem}
.title{font-size:1.7rem}
.desc{font-size:1.1rem}
}
@media(min-width:1024px){
.header{padding:1.25rem 5rem}
.main{padding:1.5rem 5rem}
.footer{padding:1.25rem 5rem}
}
@media(max-width:640px){
body{overflow:auto;}
.main{min-height:auto;padding:1.25rem 1rem;}
.error-code{font-size:3.75rem;}
.btn{padding:0.65rem 1rem;font-size:0.85rem;}
}
</style>
